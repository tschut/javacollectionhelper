<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="answerstyle.css" />
		
	<g:FlowPanel>
		<g:FlowPanel styleName="{style.arrow}">
			<g:Image url="images/arrowdown.png" />
			<g:Anchor ui:field="finalAnswer" styleName="{style.answer}" target="_blank"></g:Anchor>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 